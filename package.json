{
  "name": "@amitkshirsagar/gcode-toolpath",
  "version": "0.0.3",
  "description": "Gcode to Vector generation",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist",
    "docs"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:amitkshirsagar13/gcode-toolpath.git"
  },
  "engines": {
    "node": ">=4"
  },
  "keywords": [
    "gcode",
    "nc",
    "gc",
    "gcode-toolpath",
    "gcode2toolpath",
    "gcode2vector"
  ],
  "scripts": {
    "babel-version": "babel --version",
    "build": "rm -rf dist/ && prettier --write src/ && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJs --outDir dist/cjs",
    "build:types": "npx tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "coverage": "ts-jest --coverage",
    "prepublish": "npm run build && npm test",
    "publish-public": "npm publish --access public",
    "test": "jest",
    "test:watch": "ts-jest --watch",
    "type-check": "npx tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "author": "amit.kshirsagar.13@gmail.com",
  "homepage": "https://github.com/amitkshirsagar13/editor-components",
  "dependencies": {
    "core-js": "^3.27.1",
    "events": "^3.3.0",
    "fs": "^0.0.1-security",
    "stream": "^0.0.3",
    "timers": "^0.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/sinon": "^10.0.13",
    "babel-loader": "^9.1.2",
    "chai": "^4.3.7",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "mocha": "^10.2.0",
    "prettier": "^2.8.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
