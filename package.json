{
  "name": "gcode-toolpath",
  "version": "0.0.1",
  "description": "Gcode to Vector generation",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist",
    "docs"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:amitkshirsagar13/gcode-toolpath.git"
  },
  "engines": {
    "node": ">=4"
  },
  "keywords": [
    "gcode",
    "nc",
    "gc",
    "gcode-toolpath",
    "gcode2toolpath",
    "gcode2vector"
  ],
  "scripts": {
    "babel-version": "babel --version",
    "build": "rm -rf dist/ && prettier --write src/ && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJs --outDir dist/cjs",
    "build:types": "npx tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "coverage": "jest --coverage",
    "prepublish": "npm run build && npm test",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "npx tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "author": "amit.kshirsagar.13@gmail.com",
  "homepage": "https://github.com/amitkshirsagar13/editor-components",
  "dependencies": {
    "core-js": "^3.27.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/cli": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.2.5",
    "babel-loader": "^9.1.2",
    "jest": "^29.3.1",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
